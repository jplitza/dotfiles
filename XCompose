include "%L"

# arrows
<Multi_key> <asciicircum> <bar>	: "↑"
<Multi_key> <bar> <asciicircum>	: "↑"
<Multi_key> <equal> <less>	: "⇐"
<Multi_key> <equal> <greater>	: "⇒"
<Multi_key> <v> <bar>		: "↓"
<Multi_key> <bar> <v>		: "↓"
<Multi_key> <less> <greater>	: "↔"

# math symbols
<Multi_key> <c> <minus>		: "∈"
<Multi_key> <minus> <c>		: "∈"
<Multi_key> <C> <minus>		: "∈"
<Multi_key> <minus> <C>		: "∈"
<Multi_key> <slash> <c> <minus>	: "∉"
<Multi_key> <slash> <minus> <c>	: "∉"
<Multi_key> <slash> <C> <minus>	: "∉"
<Multi_key> <slash> <minus> <C>	: "∉"
<Multi_key> <slash> <less>	: "≮"
<Multi_key> <less> <slash>	: "≮"
<Multi_key> <slash> <greater>	: "≯"
<Multi_key> <greater> <slash>	: "≯"
<Multi_key> <V> <minus>		: "∀"
<Multi_key> <minus> <V>		: "∀"
<Multi_key> <v> <minus>		: "∀"
<Multi_key> <minus> <v>		: "∀"
<Multi_key> <E> <E>		: "∃"
<Multi_key> <v> <v>		: "∨"
<Multi_key> <o> <r>		: "∨"
<Multi_key> <a> <n> <d>		: "∧"

# smileys
<Multi_key> <semicolon> <parenright>	: "😉"
<Multi_key> <semicolon> <P>		: "😜"
<Multi_key> <colon> <P>			: "😛"
<Multi_key> <colon> <D>			: "😃"
<Multi_key> <colon> <O>			: "😲"
<Multi_key> <X> <D>			: "😆"
<Multi_key> <colon> <slash>		: "😕"
<Multi_key> <colon> <backslash>		: "😕"
<Multi_key> <colon> <bar>		: "😐"
<Multi_key> <asciicircum> <asciicircum>	: "😊"
<Multi_key> <colon> <asterisk>		: "💋"
<Multi_key> <semicolon> <asterisk>		: "😘"
<Multi_key> <greater> <colon> <parenleft>	: "😠"
<Multi_key> <X> <X> <X>			: "💋"
<Multi_key> <less> <3>			: "❤"
<Multi_key> <T> <U>			: "👍"
<Multi_key> <T> <D>			: "👎"
